{
  "name": "busim-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 15'",
    "lint": "eslint .",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "format": "prettier src/**/*.{js,jsx,ts,tsx} --write",
    "start": "react-native start",
    "commit": "git-cz",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd ..",
    "postinstall": "npx patch-package && husky"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --cache --write",
      "eslint --cache --cache-location ./.cache/.eslintcache --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.6.4",
    "@invertase/react-native-apple-authentication": "^2.3.0",
    "@react-native-camera-roll/camera-roll": "^7.8.3",
    "@react-native-kakao/core": "^2.2.5",
    "@react-native-kakao/user": "^2.2.5",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@tanstack/react-query": "^5.53.1",
    "class-variance-authority": "^0.7.0",
    "i18next": "^23.14.0",
    "ky": "^1.7.1",
    "nativewind": "^2.0.11",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.3.1",
    "react-i18next": "^15.0.1",
    "react-native": "0.75.2",
    "react-native-gesture-handler": "^2.18.1",
    "react-native-image-picker": "^7.1.2",
    "react-native-mmkv": "^3.0.2",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "^3.15.1",
    "react-native-safe-area-context": "^4.10.9",
    "react-native-screens": "3.35.0-rc.1",
    "react-native-svg": "^15.6.0",
    "react-native-webview": "^13.12.1",
    "tailwind-merge": "^2.5.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.25.4",
    "@babel/runtime": "^7.25.6",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@commitlint/cz-commitlint": "^19.4.0",
    "@react-native/babel-preset": "0.75.2",
    "@react-native/eslint-config": "0.75.2",
    "@react-native/metro-config": "^0.75.2",
    "@react-native/typescript-config": "0.75.2",
    "@tanstack/eslint-plugin-query": "^5.53.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.1",
    "@types/react": "^18.3.5",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
