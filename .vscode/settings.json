{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.requireConfig": true
}
